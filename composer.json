{
  "name": "kak/grid",
  "description": "GridView, TreeGrid widgets for Yii2",
  "type": "library",
  "keywords": [
    "yii2",
    "GridView",
    "TreeGrid"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/sanchezzzhak/kak-grid/issues?state=open",
    "source": "https://github.com/sanchezzzhak/kak-grid"
  },
  "authors": [
    {
      "name": "Tutik Alexsandr",
      "email": "sanchezzzhak@ya.ru"
    }
  ],
  "require": {
    "php": "~7.1 | ~8.1",
    "yiisoft/yii2": "*",
    "box/spout": "2.7.3",
    "bower-asset/jquery-resizable-columns": "^0.2.3",
    "bower-asset/store.js": "~2.0"
  },
  "require-dev": {
    "codeception/codeception": "4.1.13",
    "codeception/module-asserts": "1.3",
    "codeception/module-yii2": "1.1"
  },
  "autoload": {
    "psr-4": {
      "kak\\widgets\\grid\\": "src"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
